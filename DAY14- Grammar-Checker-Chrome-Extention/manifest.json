{
    "manifest_version": 3,
    "name": "Grammar Checker",
    "description": "Check the grammar of any text or paragraph using the OpenAI API",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "script.js"
        ]
      }
    ]
  }  